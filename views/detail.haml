#details{:'ng-show' => 'detailData'}
  %h3 
    Paket: {{detailData.id}}
  .row
    .span3
      Avsändare: {{detailData.sender}}
    .span3
      Mottagare: {{detailData.reciever}}
  %table#events.table
    %thead
      %tr
        %th Datum
        %th Plats
        %th Händelse
    %tbody
      %tr{:'ng-repeat' => 'event in detailData.events'}
        %td {{event.date}}
        %td {{event.location}}
        %td {{event.message}}
        
.progress.progress-striped.active{:'ng-hide' => 'detailData'}
  .bar{:style => 'width: 100%'}
